<ESRI.Configuration xmlns="http://schemas.esri.com/Desktop/AddIns" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Name>ArcGIS AI Assistant</Name>
  <AddInID>{12345678-1234-1234-1234-123456789012}</AddInID>
  <Description>AI-powered assistant for ArcGIS Pro using Gemini</Description>
  <Version>1.0</Version>
  <Image>Images\icon.png</Image>
  <Author>Your Name</Author>
  <Company>Your Company</Company>
  <Date>1/18/2026</Date>
  <Targets>
    <Target name="Desktop" version="3.0" />
  </Targets>
  <AddIn language="PYTHON" library="ai_tool.py" namespace="ai_tool">
    <ArcMap>
      <Commands>
        <Tool caption="AI Assistant" 
              category="AI Tools" 
              class="AIAssistantTool" 
              id="ai_tool_AIAssistantTool" 
              image="Images\icon.png" 
              message="Click to open AI Assistant" 
              tip="AI Assistant">
          <Help heading="AI Assistant">
            Sends commands to Gemini AI for code generation
          </Help>
        </Tool>
      </Commands>
      <Toolbars>
        <Toolbar caption="AI Assistant" 
                 category="AI Tools" 
                 id="ai_tool_AIAssistantToolbar" 
                 showInitially="true">
          <Items>
            <Tool refID="ai_tool_AIAssistantTool" />
          </Items>
        </Toolbar>
      </Toolbars>
    </ArcMap>
  </AddIn>
</ESRI.Configuration>
